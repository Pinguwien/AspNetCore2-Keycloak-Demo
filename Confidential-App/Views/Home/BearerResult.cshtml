@{
    ViewData["Title"] = "Bearer Call";
}

<div class="text-center">
    <h1 class="display-4">Bearer Calls</h1>

    <div class="form-group row">
        <label for="userId" class="col-sm-2 col-form-label">Result from call one:</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="userId" value="@Model.MessageOne" disabled=""></input>
        </div>
    </div>

    <div class="form-group row">
        <label for="username" class="col-sm-2 col-form-label">Result from call two:</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="username" value="@Model.MessageTwo" disabled=""></input>
        </div>
    </div>
    
</div>